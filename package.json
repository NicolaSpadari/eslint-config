{
  "name": "@spada/eslint-config",
  "version": "0.3.3",
  "license": "MIT",
  "author": "Nicola Spadari <spadari.nicola@gmail.com> (https://github.com/NicolaSpadari/)",
  "description": "My custom ESlint config for Typescript and Vue projects, inspired by @antfu",
  "keywords": [
    "eslint",
    "config",
    "spada",
    "lint"
  ],
  "scripts": {
    "publish:patch": "npm version patch --no-git-tag-version && npm publish",
    "publish:minor": "npm version minor --no-git-tag-version && npm publish",
    "publish:major": "npm version major --no-git-tag-version && npm publish"
  },
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NicolaSpadari/eslint-config.git"
  },
  "main": "index.js",
  "dependencies": {
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsonc": "^2.2.1",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint-plugin-vue": "^9.0.1"
  },
  "peerDependencies": {
    "eslint": ">=8.16.0",
    "typescript": ">=4.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "eslint": "^8.16.0"
  }
}
